  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "run-s dev:server:up prisma:init-local dev:server:start",
    "dev:backend": "run-s dev:server:up prisma:init-local dev:watch-entries",
    "dev:server:start": "run-p nextdev dev:watch-entries",
    "dev:watch-entries": "dotenv -e .env.local -- ts-node --project tsconfig.cjs.json src/script/watchEntries.ts",
    "dev:server:up": "dotenv -e .env.local -- ts-node --project tsconfig.cjs.json src/script/upLocalServers.ts",
    "prisma:reset-local": "dotenv -e .env.local -- prisma migrate reset",
    "prisma:init-local": "dotenv -e .env.local -- prisma migrate dev --name init",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:format": "prisma format",
    "prisma:studio": "dotenv -e .env.local -- prisma studio",
    "nextdev": "dotenv -e .env.local -e .env -- next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "tscheck": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "packageManager": "yarn@1.22.5",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@graphql-codegen/cli": "1.21.5",
