    "ts-node": "^9.1.1",
    "typescript": "4.1.3",
    "unified": "^9.2.1",
    "unist-util-visit": "2.0.3"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@chakra-ui/icons": "^1.0.13",
    "@chakra-ui/react": "^1.6.3",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mapbox/rehype-prism": "^0.6.0",
    "@prisma/client": "^2.24.1",
